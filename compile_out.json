    "trace.Participants": {
      "kind": "entity",
      "elements": {
        "ID": {
          "key": true,
          "type": "cds.UUID"
        },
        "name": {
          "type": "cds.String",
          "length": 255
        },
        "role": {
          "type": "cds.String",
          "enum": {
            "Manufacturer": {},
            "Distributor": {},
            "Pharmacy": {},
            "Regulator": {}
          }
        },
        "address": {
          "type": "cds.String",
          "length": 120
        },
        "vkh": {
          "type": "cds.String",
          "length": 56
        },
        "isActive": {
          "type": "cds.Boolean",
          "default": {
            "val": true
          }
        }
      }
    },
    "trace.Batches": {
      "kind": "entity",
      "elements": {
        "ID": {
          "key": true,
          "type": "cds.UUID"
        },
        "batchNumber": {
          "type": "cds.String",
          "length": 100
        },
        "product": {
          "type": "cds.String",
          "length": 255
        },
        "manufacturer": {
          "type": "cds.Association",
          "target": "trace.Participants",
          "keys": [
            {
              "ref": [
                "ID"
              ]
            }
          ]
        },
        "currentHolder": {
          "type": "cds.Association",
          "target": "trace.Participants",
          "keys": [
            {
              "ref": [
                "ID"
              ]
            }
          ]
        },
        "status": {
          "type": "cds.String",
          "enum": {
            "DRAFT": {},
            "MINTED": {},
            "IN_TRANSIT": {},
            "DELIVERED": {},
            "RECALLED": {}
          }
        },
        "originPayload": {
          "type": "cds.LargeString"
        },
        "onChainAsset": {
          "type": "cds.Composition",
          "cardinality": {
            "max": 1
          },
          "target": "trace.OnChainAssets",
          "on": [
            {
              "ref": [
                "onChainAsset",
                "batch"
              ]
            },
            "=",
            {
              "ref": [
                "$self"
              ]
            }
          ]
        },
        "proofEvents": {
          "type": "cds.Composition",
          "cardinality": {
            "max": "*"
          },
          "target": "trace.ProofEvents",
          "on": [
            {
              "ref": [
                "proofEvents",
                "batch"
              ]
            },
            "=",
            {
              "ref": [
                "$self"
              ]
            }
          ]
        },
        "createdAt": {
          "@cds.on.insert": {
            "=": "$now"
          },
          "type": "cds.Timestamp"
        },
        "modifiedAt": {
          "@cds.on.insert": {
            "=": "$now"
          },
          "@cds.on.update": {
            "=": "$now"
          },
          "type": "cds.Timestamp"
        }
      }
